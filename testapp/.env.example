# GolemBase Connection Configuration
# Copy this file to .env and fill in your actual values

# Required: Your private key for GolemBase authentication
PRIVATE_KEY=your_private_key_here

# Optional: GolemBase RPC and WebSocket URLs (defaults shown)
RPC_URL=https://ethwarsaw.holesky.golemdb.io/rpc
WS_URL=wss://ethwarsaw.holesky.golemdb.io/rpc/ws

# Optional: Application and schema identifiers
APP_ID=testapp
SCHEMA_ID=testapp_schema

# Alternative: Complete connection URL (if set, overrides individual parameters above)
# GOLEMBASE_DATABASE_URL=golembase:///testapp_schema?rpc_url=https://ethwarsaw.holesky.golemdb.io/rpc&ws_url=wss://ethwarsaw.holesky.golemdb.io/rpc/ws&private_key=your_private_key&app_id=testapp